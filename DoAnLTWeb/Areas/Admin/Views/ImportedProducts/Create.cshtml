@model DoAnLTWeb.Models.ImportedProduct

<form asp-action="Create">
    <div class="form-group">
        <label asp-for="IdimportedProducts" class="control-label"></label>
        <select asp-for="IdimportedProducts" class="form-control" asp-items="@(new SelectList(ViewBag.ProductIds, "Value", "Text"))"></select>
    </div>
    <div class="form-group">
        <label asp-for="Idstaff" class="control-label"></label>
        <select asp-for="Idstaff" class="form-control" asp-items="@(new SelectList(ViewBag.StaffIds, "Value", "Text"))"></select>
    </div>
    <!-- Other input fields for the ImportedProduct entity -->
    <button type="submit" class="btn btn-primary">Create</button>
</form>
